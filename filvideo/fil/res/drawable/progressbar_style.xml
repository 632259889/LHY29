<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="0.0dip" />
            <solid android:color="@color/progressbar_background" />
            <gradient android:startColor="@color/progressbar_background" android:endColor="@color/progressbar_background" android:angle="0.0" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="0.0dip" />
                <solid android:color="@color/progressbar_secondary" />
                <gradient android:startColor="@color/progressbar_secondary" android:endColor="@color/progressbar_secondary" android:angle="0.0" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress" android:drawable="@drawable/seekbar_progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="0.0dip" />
                <solid android:color="@color/progressbar_progress" />
                <gradient android:startColor="@color/theme_color" android:endColor="@color/theme_color" android:angle="0.0" />
            </shape>
        </clip>
    </item>
</layer-list>