<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <gradient android:startColor="#ff4af7ac" android:endColor="#ff24eb73" android:angle="-90.0" android:centerX="0.5" android:centerY="0.5" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#aa4af7ac" android:endColor="#aa24eb73" android:angle="-90.0" android:centerX="0.5" android:centerY="0.5" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
</selector>